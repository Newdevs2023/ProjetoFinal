{{#section 'styles'}}
<link rel="stylesheet" href="css/contato.css">
{{/section}}

<table>
    <thead>
        <tr>
            <th>ID</th>
            <th class="lanche">Lanche</th>
            <th class="descricao">Descrição</th>
            <th class="preco">Preço</th>
            <th class="add"></th>
        </tr>
    </thead>

    {{#each dados }}
        <tr>
            <td class="lanche">{{lanche}}</td>
            <td class="descricao">{{descricao}}</td>
            <td class="preco">{{preco}}</td>
            <td><a method="get" href="/produtos/editar/{{id}}"><input type="button" value="editar"></a><a method="get" href="/produtos/excluir/{{id}}"><input type="button" value="Excluir"></a></td>
        </tr>
    {{/each}}
</table>

<a method="get" href="/acoesAdm"><input type="button" class="button" id="voltar" value="Voltar"></a>